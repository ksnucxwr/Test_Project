{
  "name": "electron-auto-update-demo",
  "version": "1.0.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder  --publish always"
  },
  "build": {
    "appId": "com.example.autoupdate",
    "publish": [
      {
        "provider": "github",
        "owner": "ksnucxwr",
        "repo": "Test_Project"
      }
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "electron-updater": "6.6.6"
  },
  "devDependencies": {
    "electron": "37.2.0",
    "electron-builder": "26.0.18"
  }
}
